CREATE COMPUTE MODULE prepare_request
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		SET Environment.flujo = 'postProducts';
		--SET OutputRoot.HTTPRequestHeader = InputRoot.HTTPInputHeader;
